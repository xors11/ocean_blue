{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "ocean-blue/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/ocean-blue/$1",
            "check": true
        },
        {
            "source": "/(.*)",
            "destination": "/ocean-blue/index.html"
        }
    ]
}